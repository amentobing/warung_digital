    )
  }

  {name == "iqiyi" && <img src="/assets/IQiyi_logo.svg.png" class="w-16 h-8 mt-2.5" alt="Iqiyi Logo" />}

  {name == "vidio" && <img src="/assets/Vidio_logo.svg.png" class="w-22 h-8 mt-2.5" alt="Vidio Logo" />}

  {name == "disney+" && <img src="/assets/Disney+_logo.svg.png" class="w-20 h-12" alt="Disney+ Logo" />}

  {name == "alight-motion" && <img src="/assets/alight-motion.png" class="w-14 h-14" alt="Alight-Motion Logo" />}

  {name == "viu" && <img src="/assets/Viu_logo.svg.png" class="w-30 h-12" alt="Viu Logo" />}

  {name == "drakor-id" && <img src="/assets/drakor-id.png" class="w-14 h-14" alt="Drakor-ID Logo" />}

  {name == "bstation" && <img src="/assets/bstation.png" class="w-14 h-14" alt="BStation Logo" />}

  {name == "dramabox" && <img src="/assets/dramabox.png" class="w-14 h-14" alt="DramaBox Logo" />}

  {name == "qris" && <img src="/assets/QRIS_logo.svg.png" class="w-28 h-10" alt="DramaBox Logo" />}
