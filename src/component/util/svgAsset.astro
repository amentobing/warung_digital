  {name == "youtube" && <img src="/assets/yt_logo_fullcolor_almostblack_digital.png" alt="Youtube Logo" />}

  {name == "vision" && <img src="/assets/Vision_Plus_%282021%29.svg.png" class="h-6 my-2 mt-2.5" alt="Vision+ Logo" />}

  {name == "wetv" && <img src="/assets/WeTV_logo.svg.png" class="w-full h-8 mt-2.5" alt="WeTV Logo" />}

  {name == "spotify" && <img src="/assets/Spotify_Full_Logo_RGB_Green.png" class="h-8 mt-2.5" alt="Spotify Logo" />}

