  {name == "loklok" && <img src="/assets/loklok-svgrepo-com.svg" class="w-14 h-14" alt="LokLok Logo" />}

  {name == "lightroom" && <img src="/assets/Adobe_Photoshop_Lightroom_CC_logo.svg.png" class="w-12 h-12" alt="LightRoom Logo" />}

  {name == "brainly" && <img src="/assets/Brainly_logo.svg.png" class="w-20 h-10 my-2 mt-2" alt="BrainLy Logo" />}

